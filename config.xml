<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kwtw.kw1" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>KOM With The Wind</name>
    <description>KOM With The Wind
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-inappbrowser" source="npm"/>
   <plugin name="cordova-plugin-network-information" source="npm"/>
  <plugin name="cordova-plugin-network-information" source="npm"/>
  <plugin name="cordova-plugin-payment-iap" source="npm" />
  <plugin name="cordova-plugin-geolocation" source="npm" />
  <plugin name="cordova-plugin-statusbar" source="npm" />

  <preference name="StatusBarOverlaysWebView" value="false" />
<feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="http://api.phonegap.com/1.0/device" />
  <feature name="Geolocation">
    <param name="android-package" value="org.apache.cordova.GeoBroker" />
</feature>
<icon src="icon.png"/>
